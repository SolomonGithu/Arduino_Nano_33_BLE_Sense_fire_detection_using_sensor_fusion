/**
   @brief      Check if new serial data is available

   @return     Returns number of available bytes
*/
int ei_get_serial_available(void) {
  return Serial.available();
}

/**
   @brief      Get next available byte

   @return     byte
*/
char ei_get_serial_byte(void) {
  return Serial.read();
}

